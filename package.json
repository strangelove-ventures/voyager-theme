{
  "name": "voyager-theme",
  "version": "0.0.1",
  "description": "The shareable Chakra-UI theme for Voyager projects",
  "main": "index.js",
  "scripts": {
    "build": "pnpm turbo run build",
    "watch": "pnpm turbo run watch",
    "next:dev": "pnpm turbo run dev --filter=next-app",
    "next-app": "pnpm --dir apps/next-app",
    "chakra-theme": "pnpm --dir packages/chakra-theme"
  },
  "private": true,
  "license": "MIT",
  "prettier": "@strangelove-ventures/style-guide/prettier",
  "devDependencies": {
    "@strangelove-ventures/style-guide": "^2.0.0",
    "eslint": "^8.38.0",
    "prettier": "^2.8.7",
    "turbo": "^1.9.1",
    "typescript": "^5.0.4"
  }
}
